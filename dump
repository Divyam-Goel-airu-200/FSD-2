        <!-- <div id="output"></div>
    
        <script>
            // Define the information function
            function information() {
                // Get the output container by its ID
                const outputContainer = document.getElementById("output");
                
                // Clear any previous output
                outputContainer.innerHTML = "";
    
                // Add the new output to the container
                outputContainer.innerHTML += "<strong>The information provided is:</strong><br>";
    
                // Loop through the arguments and display each one
                for (let i = 0; i < arguments.length; i++) {
                    outputContainer.innerHTML += arguments[i] + "<br>";
                }
            }
    
            // Create an object that contains the method
            const myObject = {
                showInfo: function() {
                    // Call the information function with some arguments
                    information("Hello, World!", 42, { name: "Alice", age: 30 }, [1, 2, 3, 4, 5]);
                }
            };
    
