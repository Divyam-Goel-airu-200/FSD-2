<!-- <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Call JS Function from Object</title>
</head>
<body>

    <h1>Call JavaScript Function from Object</h1>
    <button id="infoButton">Show Information</button>

    <script>
        // Define the information function
        function information() {
            console.log("The information provided is:");
            for (let i = 0; i < arguments.length; i++) {
                console.log(arguments[i]);
            }
        }

        // Create an object that contains the method
        const myObject = {
            showInfo: function() {
                // Call the information function with some arguments
                information("Hello, World!", 42, { name: "Alice", age: 30 }, [1, 2, 3, 4, 5]);
            }
        };

        // Add an event listener to the button
        document.getElementById("infoButton").addEventListener("click", function() {
            myObject.showInfo(); // Call the method from the object
        });
    </script>

</body>
</html> -->
